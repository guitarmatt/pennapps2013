<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Where2Meet.html</welcome-file>
	</welcome-file-list>

	<servlet>
    		<servlet-name>YelpServiceImpl</servlet-name>
    		<servlet-class>pennapps2013.where2meet.server.YelpServiceImpl</servlet-class>
  	</servlet>

  	<servlet-mapping>
    		<servlet-name>YelpServiceImpl</servlet-name>
    		<url-pattern>/pennapps2013.where2meet.Where2Meet/getplaces</url-pattern>
  	</servlet-mapping>

</web-app>
